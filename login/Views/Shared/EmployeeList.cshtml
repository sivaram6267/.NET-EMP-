@model IEnumerable<login.Models.Employee_details>

@{
    ViewBag.Title = "EmployeeList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Emp_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Joining_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dob)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Practice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Designation_Lancesoft)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Emp_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Joining_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dob)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salary)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Practice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Designation_Lancesoft)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", "Employee_details", new { @id = item.Emp_Id }, null) |
                @Html.ActionLink("Details", "Details", new { id = item.Emp_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Emp_Id })
            </td>
        </tr>
    }

</table>
